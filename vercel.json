{
  "version": 2,
    "headers": [
    {
      "source": "/api/(.*)"
    }
  ],
  "name": "gp-checker-api",
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
     { "src": "/api/check-website", "dest": "/api/check-website.js" }
  ],
  "github": {
    "silent": true
  }
}